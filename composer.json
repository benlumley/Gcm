{
    "name": "benlumley/gcm",
    "description": "Endroid Google Cloud Messaging (updated to newer endpoint for iOS)",
    "keywords": ["endroid", "gcm", "push", "notification", "notifications", "google", "cloud", "messaging"],
    "homepage": "https://github.com/benlumley/Gcm",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        },
        {
            "name": "Ben Lumley",
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "kriswallsmith/buzz": "~0.3"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Gcm\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
